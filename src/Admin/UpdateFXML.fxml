<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Admin.UpdateController">
   <children>
      <TextField fx:id="enterId" layoutX="165.0" layoutY="183.0" prefHeight="37.0" prefWidth="162.0" promptText="enter the student id" />
      <Button fx:id="searchForUpdate" layoutX="334.0" layoutY="183.0" mnemonicParsing="false" onAction="#searchSubject" prefHeight="37.0" prefWidth="100.0" text="search" />
      <ComboBox fx:id="one" layoutX="164.0" layoutY="87.0" prefHeight="37.0" prefWidth="271.0" promptText="--SELECT SUBJECT--" />
      <TextField fx:id="marks1" layoutX="166.0" layoutY="230.0" prefHeight="37.0" prefWidth="271.0" promptText="Marks" />
      <ComboBox fx:id="chooseClass" layoutX="164.0" layoutY="42.0" onAction="#selectClassForUpdate" prefHeight="37.0" prefWidth="271.0" promptText="--SELECT CLASS--" />
      <ComboBox fx:id="chooseTerminal" layoutX="164.0" layoutY="136.0" prefHeight="37.0" prefWidth="271.0" promptText="----SELECT TERMINAL ---" />
      <TextField fx:id="symbolnumber" layoutX="166.0" layoutY="276.0" prefHeight="37.0" prefWidth="271.0" promptText="Symbole number" />
      <Button layoutX="165.0" layoutY="327.0" mnemonicParsing="false" onAction="#saveToUpdate" prefHeight="37.0" prefWidth="271.0" text="Save" />
   </children>
</AnchorPane>
